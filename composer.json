{
    "name": "zitec/behat-extension",
    "description": "A collection of useful methods for functional testing with Behat",
    "keywords": ["behat","BDD", "test", "zitec"],
    "license":  "MIT",
    "authors": [
        {
            "name": "Marius BALTEANU",
            "email": "marius.balteanu@zitec.com"
        },
        {
            "name": "Bianca VADEAN",
            "email": "bianca.vadean@zitec.com"
        }
    ],
    "require": {
        "behat/behat": "^3.1.0",
        "behat/mink": "dev-master@dev",
        "behat/mink-extension": "^v2.2",
        "behat/mink-goutte-driver": "^1.2.1",
        "behat/mink-selenium2-driver": "v1.3.1",
        "ciaranmcnulty/behat-stepthroughextension": "1.0.0",
        "sensiolabs/behat-page-object-extension": "v2.0.1",
        "fzaninotto/faker": "^1.6.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Zitec\\ZitecExtension": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
